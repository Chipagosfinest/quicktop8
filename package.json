{
  "name": "quicktop8-backend",
  "version": "1.0.0",
  "description": "Backend for mini app using Neynar data",
  "main": "server-enhanced.js",
  "scripts": {
    "start": "node server-enhanced.js",
    "start:legacy": "node server.js",
    "dev": "nodemon server-enhanced.js",
    "dev:legacy": "nodemon server.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "node test-api.js && node scripts/validate-urls.js",
    "test:indexer": "node test-indexer.js",
    "test:frontend": "cd frontend && npm run lint && npm run build",
    "test:api": "node test-api.js",
    "test:deployment": "node test-deployment.js",
    "validate-urls": "node scripts/validate-urls.js",
    "debug": "node debug.js",
    "deploy": "bash deploy.sh",
    "deploy:test": "npm run test && npm run deploy",
    "build": "echo 'Backend is ready to deploy'",
    "lint": "echo 'No linting configured for backend'",
    "vercel-build": "echo 'Vercel build completed'"
  },
  "keywords": ["neynar", "api", "backend", "mini-app"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
} 