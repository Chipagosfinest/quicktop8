{
  "name": "quicktop8-backend",
  "version": "1.0.0",
  "description": "Backend for QuickTop8 mini app using Neynar data",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "node test-api.js",
    "test:frontend": "cd frontend && npm run lint && npm run build",
    "test:api": "node test-api.js",
    "test:deployment": "node test-deployment.js",
    "debug": "node debug.js",
    "deploy": "bash deploy.sh",
    "build": "cd frontend && npm install && npm run build",
    "lint": "echo 'No linting configured for backend'",
    "vercel-build": "npm run build"
  },
  "keywords": ["neynar", "api", "backend", "mini-app", "farcaster"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
} 