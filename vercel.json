{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "redirects": [],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/frontend/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "headers": [],
  "env": {
    "NODE_ENV": "production"
  }
} 